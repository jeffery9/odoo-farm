<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="mrp_bom_form_view_inherit_farm" model="ir.ui.view">
        <field name="name">mrp.bom.form.inherit.farm</field>
        <field name="model">mrp.bom</field>
        <field name="inherit_id" ref="mrp.mrp_bom_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='bom_line_ids']//list//field[@name='product_qty']" position="after">
                <field name="dilution_ratio" optional="show"/>
            </xpath>
        </field>
    </record>
</odoo>
