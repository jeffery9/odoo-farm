<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_farm_certificate_monitor" model="ir.cron">
            <field name="name">Farm: Certificate Expiry Monitor</field>
            <field name="model_id" ref="model_farm_certificate_type"/>
            <field name="state">code</field>
            <field name="code">model._cron_check_certificate_expiry()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>
